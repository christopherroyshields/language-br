{
  ".source.purebasic": {
    "CompilerError <string constant>": {
      "prefix": "CompilerError",
      "body": "CompilerError \"${1}\"",
      "description": "Generates an error, as if it was a syntax error and display the associated message.",
      "descriptionMoreURL": "http://www.purebasic.com/documentation/reference/compilerdirectives.html"
    },
    "CompilerIf … CompilerEndIf": {
      "prefix": "CompilerIf",
      "body": "CompilerIf ${1}\n\n\t${2}\n\nCompilerEndIf",
      "description": "If the <constant expression> result is true, the code inside the CompilerIf will be compiled, else it will be totally ignored.",
      "descriptionMoreURL": "http://www.purebasic.com/documentation/reference/compilerdirectives.html"
    },
    "CompilerEndIf": {
      "prefix": "CompilerEndIf",
      "body": "CompilerEndIf",
      "descriptionMoreURL": "http://www.purebasic.com/documentation/reference/compilerdirectives.html"
    },
    "CompilerSelect": {
      "prefix": "CompilerSelect",
      "body": "CompilerSelect ${1:<numeric constant>}\n\tCompilerCase ${2:<numeric constant>}\n\t\t${3}\nCompilerEndSelect",
      "description": "Works like a regular Select : EndSelect except that only one numeric value is allowed per case.",
      "descriptionMoreURL": "http://www.purebasic.com/documentation/reference/compilerdirectives.html"
    },
    "CompilerWarning <string constant>": {
      "prefix": "CompilerWarning",
      "body": "CompilerWarning \"${1}\"",
      "description": "Generates a warning, as if it was a syntax error and display the associated message.",
      "descriptionMoreURL": "http://www.purebasic.com/documentation/reference/compilerdirectives.html"
    },
    "Debug Expression": {
      "prefix": "Debug",
      "body": "Debug ${1:Expression}\n$2"
    },
    "Data section": {
      "prefix": "DataSection",
      "body": "DataSection\n\t${1}\nEndDataSection"
    },
    "Declare[.<type>] name(<parameter1[.<type>]> [, <parameter2[.<type>] [= DefaultValue]>, ...])": {
      "prefix": "Declare",
      "body": "Declare ${1}"
    },
    "EnableASM": {
      "prefix": "EnableASM",
      "body": "EnableASM\n${1}"
    },
    "EnableExplicit": {
      "prefix": "EnableExplicit",
      "body": "EnableExplicit\n${1}"
    },
    "EndIf": {
      "prefix": "EndIf",
      "body": "EndIf"
    },
    "Enumeration block": {
      "prefix": "Enumeration",
      "body": "Enumeration${1}\n\n\t${2}\n\nEndEnumeration"
    },
    "EndEnumeration": {
      "prefix": "EndEnumeration",
      "body": "EndEnumeration"
    },
    "Foreach … Next": {
      "prefix": "ForEach",
      "body": "ForEach ${1:LinkedList}()\n\n\t${2}\n\nNext"
    },
    "IncludeFile": {
      "prefix": "Inc",
      "body": "IncludeFile \"${1}\""
    },
    "If … EndIf": {
      "prefix": "If",
      "body": "If ${1}\n\n\t${2}\n\nEndIf"
    },
    "Procedure implementation": {
      "prefix": "Procedure",
      "body": "Procedure${1} ${2:Unnamed}(${3})\n\n\t${4:; Not implemented yet.}\n\nEndProcedure"
    },
    "ProcedureDLL implementation": {
      "prefix": "ProcedureDLL",
      "body": "ProcedureDLL${1} ${2:Unnamed}(${3})\n\n\t${4:; Not implemented yet.}\n\nEndProcedure"
    },
    "Procedure return": {
      "prefix": "",
      "body": "ProcedureReturn ${1}"
    },
    "Repeat … ForEver": {
      "prefix": "Re",
      "body": "Repeat\n\n\t{$1}\n\nForEver"
    },
    "Select <expression>": {
      "prefix": "Select",
      "body": "Select ${1:<expression>}\n\tCase ${2:<expression>}\n\t\t${3}\nEndSelect",
      "description": "Select provides the ability to determine a quick choice.",
      "descriptionMoreURL": "http://www.purebasic.com/documentation/reference/select_endselect.html"
    },
    "While … Wend": {
      "prefix": "While",
      "body": "While $1\n\n  $2\n\nWend"
    },
    "Structure … EndStructure": {
      "prefix": "Structure",
      "body": "Structure $1\n  $2\nEndStructure"
    },
    "Repeat … Until …": {
      "prefix": "Re",
      "body": "Repeat\n\n\t{$2}\n\nUntil ${1}"
    },
    "XIncludeFile": {
      "prefix": "XInc",
      "body": "XIncludeFile \"${1}\""
    }
  }
}
